{
  "name": "csdn-extension",
  "version": "3.0.1",
  "description": "C笔记",
  "author": "csdn",
  "license": "MIT",
  "engines": {
    "node": ">= 10"
  },
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.vue src",
    "prettier": "prettier \"src/**/*.{js,vue}\"",
    "prettier:write": "npm run prettier -- --write",
    "build:normalEntry": "cross-env NODE_ENV=production webpack --hide-modules",
    "build:independentEntry": "cross-env NODE_ENV=production independentEntry=true webpack --hide-modules",
    "build": "npm run build:normalEntry && npm run build:independentEntry",
    "dev": "cross-env HMR=true NODE_ENV=development webpack --sourcemap --watch",
    "build:zip": "npm run build && node scripts/build-zip.js",
    "release": "node scripts/release.js"
  },
  "dependencies": {
    "ant-design-vue": "^1.7.8",
